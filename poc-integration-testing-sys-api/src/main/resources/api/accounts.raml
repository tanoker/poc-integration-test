#%RAML 1.0 DataType
type: array
items:
  type: object
  properties:
    name: string
    email: string
    accountNumber: string
    currency:
      type: string
      enum:
        - RUR
        - EUR
        - USD
    billingCity: string
    billingCountry: string
    billingStreet: string
    billingZipCode: string
    mobile: string